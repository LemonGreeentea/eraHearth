@c___onEnemyTurn

	; 같은 위치에 있는 경우
	if isMasterInRange()
		if RAND:100 < CFLAG:TARGET:성공격빈도
			TFLAG:이번턴성공격 = 1
			trycCALLform c{CFLAG:TARGET:캐릭터번호}onSexualAttack
				;
			catch
				CALL fallbackOnSexualAttack
			endcatch
		else
			TFLAG:이번턴성공격 = 0
			trycCALLform c{CFLAG:TARGET:캐릭터번호}onNormalAttack
				;
			catch
				CALL fallbackOnNormalAttack
			endcatch
		endif
	; 다른 위치에 있을 경우
	else
		CALL processMoveEnemy
	endif
