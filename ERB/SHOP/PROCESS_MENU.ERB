@processMenu

	; This commands are always OK
	if RESULT != 201 && RESULT != 202 && RESULT < 800 || RESULT == 901
		if !isMasterFree()
			return 0
		endif
	endif

	; ------------------------------------------ 
	;  운영 탭
	;  - 연구말고는 항상 이용할 수 있는 곳
	;  
	;
	; ------------------------------------------
	; 여관관리
	IF RESULT == 101
		CALL INN_MANAGE
	; 
	ELSEIF RESULT == 102 && !hasHuntQuest() && !FLAG:원정중
		CALL newQuestList
	elseif RESULT == 103 && isMasterFree() && hasQuest()
		CALL cancelQuest
	elseif RESULT == 104 && hasDeliverQuest() && hasDeliveryItems()
		ITEM:(FLAG:퀘스트대상) -= (FLAG:퀘스트값)
		CALL stepQuest

	; CHARACTER MENU
	elseif RESULT == 201
		CALL showMasterInfo
		RESULT = 0
	elseif RESULT == 202
		CALL showInventory
		RESULT = 0
	elseif RESULT == 203
		CALL showMasterEquips
		RESULT = 0
	elseif RESULT == 204
		CALL showSkills
		RESULT = 0

	; SHOP MENU
	elseif RESULT == 301 && isMasterFree() && !FLAG:원정중
		CALL itemShop
	elseif RESULT == 302 && isMasterFree() && !FLAG:원정중
		CALL backStreet
	elseif RESULT == 303 && isMasterFree() && !FLAG:원정중
		CALL cathedral
	elseif RESULT == 304 && isMasterFree() && !FLAG:원정중
		CALL alchemyStreet

	; COLLECTION MENU
	elseif RESULT == 701
		CALL showBirthPedia

	; SYSTEM MENU
	elseif RESULT == 801
		saveGame
		RESULT = 0
	elseif RESULT == 802
		loadGame
	elseif RESULT == 803
		CALL configMenu
		RESULT = 0

	ELSEIF RESULT == 805
		CALL TESTMENU01
	ELSEIF RESULT == 806
		CALL TESTMENU02
	ELSEIF RESULT == 807
		CALL TESTMENU03

	ELSEIF RESULT == 808
		CALL DEVROOMMENU

	; TURN END
	elseif RESULT == 900 && !FLAG:원정중
		begin turnend
	elseif RESULT == 901 && !FLAG:원정중
		CALL inn
	; WRONG COMMAND
	else
		RESULT = 0
	endif

	return RESULT

@saveInfo
	PUTFORM {DAY,3,RIGHT}일차 %NAME:MASTER, 14, LEFT%
